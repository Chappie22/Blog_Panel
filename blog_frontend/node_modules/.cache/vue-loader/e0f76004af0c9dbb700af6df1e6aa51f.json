       <div class=\"delete-comment\" title=\"Delete Comment\" v-on:click=\"{comments.splice(index, 1)}\">&times;</div>\r\n                        </div>\r\n                        <div class=\"comment-text\">\r\n                            <p style=\"white-space: pre-line\" v-html=\"comment.text\"></p>\r\n                        </div>\r\n                        <div class=\"comment-meta\">\r\n                            <span class=\"comment-date\">{{comment.date}}</span><span class=\"comment-time\">{{comment.time}}</span>\r\n                            <a href=\"#comments-form\" v-scroll-to=\"'#comments-form'\">Reply</a>\r\n                        </div>\r\n                    </li>\r\n                </transition-group>\r\n\r\n                <div id=\"comments-form\" class=\"comments-form\">\r\n                    <h2>Leave a Comment</h2>\r\n                    <form v-on:submit.prevent=\"addComment\">\r\n                        <div class=\"field-wrapper\">\r\n                            <label for=\"user\">Name</label>\r\n                            <input type=\"text\" name=\"user\" v-model=\"newComment.user\" />\r\n                        </div>\r\n\r\n                        <div class=\"field-wrapper\">\r\n                            <label for=\"comment-text\">Comment</label>\r\n                            <textarea name=\"comment-text\" v-model=\"newComment.text\"></textarea>\r\n                        </div>\r\n\r\n                        <div class=\"field-wrapper\">\r\n                            <input class=\"btn\" type=\"submit\" value=\"Add Comment\" />\r\n                        </div>\r\n\r\n                    </form>\r\n                </div>\r\n\r\n            </footer>\r\n        </article>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"post\",\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    body {\r\n        color: #666;\r\n    }\r\n\r\n    article {\r\n        max-width: 600px;\r\n        margin: